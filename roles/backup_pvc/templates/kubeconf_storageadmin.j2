apiVersion: v1
clusters:
- cluster:
    certificate-authority: /var/run/secrets/kubernetes.io/serviceaccount/ca.crt
    server: https://kubernetes.default:443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: apb-user
  name: /kubernetes/apb-user
- context:
    cluster: kubernetes
    user: {{ pv_srv_acct }}
  name: /kubernetes/storage-admin
current-context: /kubernetes/apb-user
kind: Config
preferences: {}
users:
- name: apb-user
  user:
    tokenFile: /var/run/secretes/kubernetes.io/serviceaccount/token
- name: {{ pv_srv_acct }}
  user:
    token: {{ pv_srv_acct_token }}
#    tokenFile: /etc/apb-secrets/apb-backup-nfs-conf/pv_srv_acct_token
